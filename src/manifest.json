{
	"name": "ezttrss",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "A simple way to keep up with your Tiny Tiny RSS feeds",
	"background": {
		"page": "background.html"
	}, "browser_action": {
		"default_icon": "rss_19.png"
	}, "icons": {
		"19": "rss_19.png",
		"48": "rss_48.png",
		"128": "rss_128.png"
	},
	"permissions": [
		"*://*/*"
	]
}
